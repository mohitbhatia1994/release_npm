{
    "non-interactive": false,
    "dry-run": false,
    "verbose": true,
    "force": false,
    "pkgFiles": ["version.json", "package.json"],
    "increment": "patch",
    "prereleaseId": "-SNAP",
    "commitMessage": "Bumping version to %s",
    "tagName": "%s",
    "tagAnnotation": "Release %s",
    "buildCommand": false,
    "changelogCommand": "git log --pretty=format:'* %s (%h)' [REV_RANGE]",
    "requireCleanWorkingDir": false,
    "src": {
        "pushRepo": null,
        "beforeStartCommand": false,
        "beforeStageCommand": false,
        "afterReleaseCommand": false
    },
    "dist": {
        "repo": false,
        "stageDir": ".stage",
        "baseDir": "dist",
        "files": ["**/*"],
        "pkgFiles": null,
        "beforeStageCommand": false,
        "afterReleaseCommand": false
    },
    "npm": {
        "publish": false,
        "publishPath": ".",
        "tag": "latest",
        "private": true,
        "forcePublishSourceRepo": false
    },
    "github": {
        "release": false,
        "releaseName": "Release %s",
        "tokenRef": "GITHUB_TOKEN"
    }
}
